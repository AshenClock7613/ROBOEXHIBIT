        <progettoMuseo:tipoStanza xml:lang="it">Corridoio</progettoMuseo:tipoStanza>
        <accessibilitàStanza rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1</accessibilitàStanza>
        <rdfs:label xml:lang="it">Corridoio</rdfs:label>
        <rdfs:label xml:lang="en">Hallway</rdfs:label>
    </owl:NamedIndividual>
    


