    <!-- http://www.semanticweb.org/lucreziamosca/ontologies/progettoMuseo#museo -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/lucreziamosca/ontologies/progettoMuseo#museo">
        <rdf:type rdf:resource="http://www.semanticweb.org/lucreziamosca/ontologies/progettoMuseo/Museo"/>
